    {
        "title": "Website Performance Optimization",
        "description": "Our optimization solutions ensure your website or web app delivers a fast, smooth user experience, crucial for user satisfaction and conversion rates.",
        "targetAudience": "Enterprises, Government Entities"
    },
    {
        "title": "User Research & UX/UI Design",
        "description": "We employ a user-centered approach, conducting detailed user research to inform our UX/UI design process, resulting in digital experiences that resonate with your audience.",
        "targetAudience": "Enterprises, Government Entities"
    },
    {
        "title": "Web Accessibility Solutions",
        "description": "Our experts ensure your digital platforms are accessible to all users, complying with WCAG guidelines to enhance usability and meet legal requirements.",
        "targetAudience": "Enterprises, Government Entities"
    },
    {
        "title": "UX/UI Design Systems",
        "description": "We build comprehensive design systems with Tailwind, enabling consistent, efficient, and scalable design across your digital products.",
        "targetAudience": "Enterprises, Government Entities"
