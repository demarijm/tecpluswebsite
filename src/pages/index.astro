        "title": "Enterprise Web App Development",
        "description": "Our experienced team leverages modern frameworks like React and Next.js to deliver robust, scalable web applications tailored to meet your enterprise's unique needs.",
        "targetAudience": "Enterprises, Government Entities"
    },
    {
        "title": "AI and Machine Learning Solutions",
        "description": "Harnessing the power of Python, we build AI-driven solutions, including machine learning models, data analytics tools, and predictive systems.",
        "targetAudience": "Enterprises, Government Entities"
    },
    {
        "title": "High Perfomance Distributed Backend Solutions",
        "description": "We employ Golang's simplicity, efficiency, and strong support for concurrent programming to build powerful and scalable backend systems.",
        "targetAudience": "Enterprises, Government Entities"
    },
    {
        "title": "Mobile Application Development",
        "description": "Utilizing React Native, we create feature-rich, cross-platform mobile applications that provide a seamless and engaging user experience.",
        "targetAudience": "Enterprises, Government Entities"
    },
    // {
    //     "title": "CMS Integration",
    //     "description": "We specialize in integrating and customizing powerful content management systems like WordPress and Sanity, empowering you to manage your digital content effectively.",
    //     "targetAudience": "Enterprises, Government Entities"
    // },
    {
        "title": "Website Performance Optimization",
        "description": "Our optimization solutions ensure your website or web app delivers a fast, smooth user experience, crucial for user satisfaction and conversion rates.",
        "targetAudience": "Enterprises, Government Entities"
    },
    {
        "title": "User Research & UX/UI Design",
        "description": "We employ a user-centered approach, conducting detailed user research to inform our UX/UI design process, resulting in digital experiences that resonate with your audience.",
        "targetAudience": "Enterprises, Government Entities"
    },
    {
        "title": "Web Accessibility Solutions",
        "description": "Our experts ensure your digital platforms are accessible to all users, complying with WCAG guidelines to enhance usability and meet legal requirements.",
        "targetAudience": "Enterprises, Government Entities"
    },
    {
        "title": "UX/UI Design Systems",
        "description": "We build comprehensive design systems with Tailwind, enabling consistent, efficient, and scalable design across your digital products.",
        "targetAudience": "Enterprises, Government Entities"
    }
]

const featured = [
	{
		title: "Tecplus, Digital Agency with a personal touch",
